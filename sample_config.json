{
  "starters": {
    "image_load": {
      "path": "${SRC_DIR}/data/tulips.png"
    }
  },
  "imgproc": {
    "monochrome": {
      "mode": "cie_luma",
      "input": "image_load.output"
    },
    "tonemap": {
      "input": "monochrome.output"
    },
    "blur": {
      "radius": 5,
      "input": "tonemap.output"
    }
  },
  "finishers": {
    "display": {
      "input": "blur.output"
    }
  }
}