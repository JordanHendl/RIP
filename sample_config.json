{
  "dimensions": [512, 512],
  "starters": {
    "image_load": {
      "path": "${SRC_DIR}/data/test.png"
    }
  },
  "imgproc": {
    "transform": {
      "off_x": 0.0,
      "off_y": 256.0,

      "scale_x": 1.0,
      "scale_y": 1.0,
      
      "shear_x": 0.1,
      "shear_y": 0.5,

      "rotation": 0,
      "input": "image_load.output"
    },
    "blur": {
      "radius": 0,
      "input": "transform.output"
    }
  },
  "finishers": {
    "display": {
      "input": "blur.output"
    }
  }
} 