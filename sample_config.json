{
  "dimensions": [1280, 1024],
  "starters": {
    "image_load": {
      "path": "${SRC_DIR}/data/test.png"
    }
  },
  "imgproc": {
    "monochrome": {
      "mode": "intensity",
      "input": "image_load.output"
    },
    "blur": {
      "radius": 4,
      "input": "monochrome.output"
    },
    "threshold": {
      "radius": 4,
      "input": "blur.output"
    }
  },
  "finishers": {
    "display": {
      "input": "threshold.output"
    }
  }
}