{
  "starters": {
    "image_load": {
      "path": "${SRC_DIR}/data/tulips.png"
    }
  },
  "imgproc": {
    "monochrome": {
      "input": "image_load.output"
    },
    "inverse": {
      "input": "monochrome.output"
    },
    "blur": {
      "input": "inverse.output"
    }
  },
  "finishers": {
    "image_write": {
      "input": "blur.output",
      "path": "${SRC_DIR}/target/test.png"
    }
  }
}